{
  "name": "Quick open JIRA issues",
  "version": "1.0",
  "description": "Quickly opens JIRA issues",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["storage", "declarativeContent", "activeTab", "tabs", "storage"],
  "browser_action": {
    "default_popup": "popup.html",
    "options_page": "options.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "manifest_version": 2
}
